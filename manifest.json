{
  "name": "Googly Eyes",
  "description": "Add Googly Eyes to All Images on the Page",
  "version": "1.0",
  "manifest_version": 3,
  "background": {
    "service_worker": "src/background.js"
  },
  "action": {
    "default_icon": {}
  },
  "web_accessible_resources": [{
      "matches": ["<all_urls>"],
      "resources": ["src/images/*.png"]
  }],
  "permissions": ["activeTab", "scripting"]
}